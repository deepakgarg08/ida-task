prefix ab:<http://userdata/#wonderwoman> 
prefix cd: <http://www.w3.org/2001/vcard-rdf/3.0#>
select ?name ?username ?password 
	where {ab: cd:name ?name ; cd:password ?password ; cd:username ?username .}
